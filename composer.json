		"create",
		"delete",
		"rename",
		"locate",
		"filter",
		"view"
	],
	"homepage" : "https://github.com/random-cuber/contextmenu_folder",
	"license" : "Apache-2.0",
	"type" : "roundcube-plugin",
	"version" : "2.0.0",
	"authors" : [{
